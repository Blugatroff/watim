}

struct Buf { a: i32 b: i32 c: i32 d: i32 e: i32 f: i32 g: i32 h: i32 }

fn print(n: i32) {
	uninit<Buf> @buf &buf.a @buf
	uninit<Buf> @buf-reversed &buf-reversed.a @buf-reversed

	$n 0 = if {
		1 #l // length = 1
		$buf 48 store8 // put '0' in buf
	} else {
		loop {
			$n 0 = if { break }
			$buf $l +
			$n 10 % // rightmost digit
			48 + // + ascii 'a'
			store8
			$n 10 / #n // shift right in decimal
			$l 1 + #l
		}
	}
	0 @i
	loop {
		$buf-reversed $i +
		$buf $l 1 - $i - + load8
		store8
		$i 1 + #i
		$i $l = if { break }
	}
	1 $buf-reversed $l write drop
}

fn write_byte(file: i32, b: i32) {
	uninit<i32> @buf
	&buf $b store8
	$file &buf 1 write drop
}

fn parse(pt: .i32, len: i32) -> i32 {
	$pt @original-ptr
	$len @original-len
	0 @n
	loop {
		$pt load8 @d
		$d 48 ge $d 58 le and if { // 48 is ascii '0'
			$n $d 48 - + #n
		} else {
			1 "Failed to parse: '" write drop
			1 $original-ptr $original-len write drop
			1 "'" write drop
			//1 "\n" write drop
			1 10 write_byte
			1 exit
		}
		$pt 1 + #pt // advance pointer
		$len 1 - #len // reduce length
		$len 0 = if { $n break }
		$n 10 * #n
	}
}

fn dup<T>(a: T) -> T, T { $a $a }

fn main "_start" () {
	uninit<Buf> @buf &buf.a @buf
	0 $buf 32 read @nread
	$nread 0 /= if {
		$buf $nread 1 - + ~ "\n" drop ~ = if { $nread 1 - #nread }
	}
	$buf $nread parse
	dup<i32> print 
	1 "\n" write drop
	exit
}

import "./util.watim" as U

fn print_alphabet() {
    memory buf 27; // 26 letters + \n
    local i: i32
    loop {
        $i 26 = if { break }
        $i $buf + 97 $i + store8
        $i $buf + 1 + 10 store8

        1 $buf $i 2 + U.write drop
        
        $i 1 + #i
    }
}

fn main "_start" () {
    print_alphabet
}
